{
  "version": 2,
  "builds": [
    {
      "src": "backend/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/wsgi.py"
    }
  ],
  "installCommand": "pip install -r backend/requirements.txt",
  "buildCommand": "cd backend && flask db upgrade && python seed.py"
}
